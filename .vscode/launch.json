  "version": "0.2.0",
  "configurations": [
    {
      "name": "LOCAL DEVICE",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define", 
        "USE_FIREBASE_EMU=true",
      ]
    },
    {
      "name": "PRODUCTION_DEBUG",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define", 
        "USE_FIREBASE_EMU=false"
      ],
    },
    {
      "name": "PRODUCTION",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define", 
        "USE_FIREBASE_EMU=false"
      ],
      "flutterMode": "release"
    },
    {
      "name": "PROFILE w/EMULATOR",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define", 
        "USE_FIREBASE_EMU=true", 
      ],
      "flutterMode": "profile"
    },
    {
      "name": "RELEASE w/EMULATOR",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define", 
        "USE_FIREBASE_EMU=true", 
      ],
      "flutterMode": "release"
    },
  ]
}